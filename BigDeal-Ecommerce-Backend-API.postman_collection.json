            {
              "name": "Get All Products",
              "request": {
                "method": "POST",
                "header": [
                  {
                    "key": "Content-Type",
                    "value": "application/json"
                  }
                ],
                "body": {
                  "mode": "raw",
                  "raw": "{\n  \"query\": \"query { products(limit: 10, indexFrom: 0) { items { id title description type brand category price new sale discount stock images { alt src } variants { id sku size color image_id } collection { collectionName } } total totalPages hasMore currentPage } }\"\n}"
                },
                "url": {
                  "raw": "http://localhost:3001/graphql",
                  "protocol": "http",
                  "host": ["localhost"],
                  "port": "3001",
                  "path": ["graphql"]
                },
                "description": "Fetch all products with pagination (first page, 10 items per page)"
